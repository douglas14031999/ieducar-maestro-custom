    "autoload": {
        "classmap": [
            "ieducar/intranet/include",
            "ieducar/lib",
            "ieducar/modules/*/Model",
            "ieducar/modules/Avaliacao/Service",
            "ieducar/modules/Avaliacao/Views",
            "ieducar/modules/FormulaMedia/Validate",
            "ieducar/modules/RegraAvaliacao/Validators"
        ],
        "files": [
            "ieducar/intranet/include/funcoes.inc.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Custom\\": "database/custom/",
            "Database\\Seeders\\": "database/seeders/",
            "iEducar\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        },
        "classmap": [
            "tests/Unit"
        ]
    },
    "include-path": [
        "ieducar",
        "ieducar/intranet",
        "ieducar/lib",
        "ieducar/modules"
    ],
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/telescope"
            ]
        }
    },
    "scripts": {
        "format": "vendor/bin/pint",
        "openapi": "vendor/uderline/openapi-php-attributes/opag tests public/openapi.json",
        "set-permissions": "find bootstrap/cache -type d -exec chmod 777 {} + && find storage -type d -exec chmod 777 {} +",
        "new-install": [
            "@composer install",
            "@composer plug-and-play",
            "@set-permissions",
            "@php artisan key:generate --ansi",
            "@php artisan storage:link --ansi",
            "@php artisan migrate --force --ansi"
        ],
        "update-install": [
            "@composer update",
            "@composer plug-and-play",
            "@set-permissions",
            "@php artisan storage:link --ansi",
            "@php artisan migrate --force --ansi"
        ],
        "post-install-cmd": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "dex/composer-plug-and-play": true,
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "path",
            "url": "./packages/i-educar-reports-package"
        },
        {
            "type": "path",
            "url": "./packages/pre-matricula-digital"
        },
        {
            "type": "path",
            "url": "./packages/i-educar-educacenso-package"
        },
        {
            "type": "path",
            "url": "./packages/portabilis/i-educar-library-package",
            "symlink": true
        },
        {
            "type": "path",
            "url": "./packages/portabilis/i-educar-transport-package",
            "symlink": true
        }
    ]
}
